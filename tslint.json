{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single",
            "avoid-escape"
        ],
        "no-console": false,
        "object-literal-sort-keys": false,
        "no-reference": false,
        "ordered-imports": false,
        "arrow-parens": [
            true,
            "ban-single-arg-parens"
        ],
        "no-var-requires": false
    },
    "rulesDirectory": []
}