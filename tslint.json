{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single",
            "avoid-escape"
        ],
        "no-console": false,
        "object-literal-sort-keys": false,
        "no-reference": false,
        "ordered-imports": false,
        "arrow-parens": [
            true,
            "ban-single-arg-parens"
        ],
        "no-var-requires": false,
        "ban-types": false,
        "max-classes-per-file": true,
        "no-empty": false,
        "no-shadowed-variable": false
    },
    "rulesDirectory": []
}